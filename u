ask = float(input("Enter a number between 10 and 20 >>"))
while ask < 10 and ask > 20 :
  if ask > 20 :
    print("Your number is too high")
    print("")
    ask = float(input("Enter a number between 10 and 20 >>"))
  else:
    print("Your number is too low")
    ask = float(input("Enter a number between 10 and 20 >>"))
